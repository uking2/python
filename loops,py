# # print 1 to 10

# i =1  #initialization

# while i <=10: #condition
#     print(i) 
#     i += 1 #increment/decrement

# print 5 to 1
r = 5

while r >= 1:
    print(r)
    if(r == 3):
        break
    r -= 1

i =1  #initialization

while i <=10: #condition
    if (i ==5):
        i += 1
        continue #skip the current iteration
    print(i) 
    i += 1 #increment/decrement    